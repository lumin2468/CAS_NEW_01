<!-- script -->
<script>
  let btn1 = document.getElementById("btn1");
  let btn2 = document.getElementById("btn2");
  let rightBtn = document.getElementById("rightBtn");
  let profile = document.getElementById("profile");
  let master = document.getElementById("master");
  let Reports = document.getElementById("Reports");
  let Transaction = document.getElementById("Transaction");
  let dashlist = document.getElementById("list");
  let dashlist2 = document.getElementById("list2");
  let dashlist3 = document.getElementById("list3");
  let largeSidebar = document.getElementById("large-sidebar");
  let smallSidebar = document.getElementById("small-sidebar");

  master.onclick = () => {
    if (dashlist.style.display === "none") {
      dashlist.style.display = "block";
    } else {
      dashlist.style.display = "none";
    }
  };
  Reports.onclick = () => {
    if (dashlist3.style.display === "none") {
      dashlist3.style.display = "block";
    } else {
      dashlist3.style.display = "none";
    }
  };
  Transaction.onclick = () => {
    if (dashlist2.style.display === "none") {
      dashlist2.style.display = "block";
    } else {
      dashlist2.style.display = "none";
    }
  };
  btn1.onclick = () => {
    if (largeSidebar.style.display === "none") {
      largeSidebar.style.display = "block";
      smallSidebar.style.display = "none";
    } else {
      largeSidebar.style.display = "none";
      smallSidebar.style.display = "block";
    }
  };
  btn2.onclick = () => {
    if (largeSidebar.style.display === "none") {
      largeSidebar.style.display = "block";
      smallSidebar.style.display = "none";
    } else {
      largeSidebar.style.display = "none";
      smallSidebar.style.display = "block";
    }
  };

  rightBtn.onclick = () => {
    if (profile.style.display === "none") {
      profile.style.display = "block";
    } else {
      profile.style.display = "none";
    }
  };
</script>
